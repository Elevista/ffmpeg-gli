{
  "name": "ffmpeg-gli",
  "version": "0.0.1",
  "author": "Elevista <elevista@sunnyholic.com>",
  "description": "An electron-vue project",
  "license": "ISC",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node config/build.js && electron-builder",
    "build:dir": "node config/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node config/build.js",
    "dev": "node config/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src config",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src config",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config config/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config config/webpack.renderer.config.js",
    "postinstall": ""
  },
  "build": {
    "productName": "ffmpeg-gli",
    "appId": "com.elevista.ffmpeggli",
    "directories": {
      "output": "build",
      "buildResources": "static"
    },
    "files": [
      "dist/electron/**/*"
    ]
  },
  "dependencies": {
    "vue": "^2.5.16",
    "lodash": "^4.17.11",
    "mime-types": "^2.1.21",
    "moment": "^2.24.0",
    "muse-ui": "^3.0.1",
    "vuedraggable": "^2.17.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "ajv": "^6.7.0",
    "cfonts": "^2.4.0",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^4.0.2",
    "electron-builder": "^20.38.5",
    "electron-debug": "^2.1.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-vue": "^5.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "multispinner": "^0.2.1",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.6.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elevista/ffmpeg-gli.git"
  },
  "bugs": {
    "url": "https://github.com/Elevista/ffmpeg-gli/issues"
  },
  "homepage": "https://github.com/Elevista/ffmpeg-gli#readme"
}
